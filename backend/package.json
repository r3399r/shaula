{
  "name": "shaula-backend",
  "version": "0.0.1",
  "author": "Yue Chang",
  "scripts": {
    "lint:es": "eslint -c ./eslint.json src",
    "fix:es": "npm run lint:es -- --fix",
    "lint:format": "prettier --config ./prettierrc.json --list-different \"./**/*.ts\" --ignore-path .gitignore",
    "fix:format": "npm run lint:format -- --write",
    "lint": "run-p lint:*",
    "fix": "run-s fix:*",
    "clean": "del-cli dist lib -f",
    "test": "jest -c jest.config.js",
    "test:coverage": "npm run test -- --coverage --verbose",
    "build": "webpack --config webpack.config.js",
    "pre:commit": "run-s lint test:coverage build",
    "pre:dev": "run-s fix test:coverage",
    "pre:deploy": "run-s clean build"
  },
  "license": "ISC",
  "dependencies": {
    "@line/bot-sdk": "^7.5.2",
    "aws-sdk": "^2.1347.0",
    "axios": "^1.4.0",
    "bignumber.js": "^9.1.1",
    "date-fns": "^2.30.0",
    "file-type": "^18.2.1",
    "inversify": "^6.0.1",
    "lodash": "^4.17.21",
    "node-telegram-bot-api": "^0.61.0",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.4.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/lodash": "^4.14.186",
    "@types/multer": "^1.4.7",
    "@types/node-telegram-bot-api": "^0.61.5",
    "@types/pg": "^8.6.5",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "del-cli": "^5.0.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.11.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^29.3.1",
    "multer": "^1.4.5-lts.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.7.4",
    "typescript-transform-paths": "^3.3.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^5.0.1",
    "zip-webpack-plugin": "^4.0.1"
  }
}
